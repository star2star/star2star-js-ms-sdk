{
  "name": "star2star-js-ms-sdk",
  "version": "2.1.9",
  "description": "Star2Star JavaScript Micro Service SDK",
  "main": "./dist/index.js",
  "scripts": {
    "init": "mkdir dist",
    "clean": "rm -rf dist && rm package-lock.json && rm -rf node_modules",
    "test": "mocha --timeout 10000",
    "prebuild": "npm run clean && npm run init && npm install",
    "build": "babel src -d dist --presets es2015",
    "postbuild": "cp src/config.json dist/config.json",
    "doc": "documentation build src/index.js -f html -o . --config documentation.yml"
  },
  "keywords": [
    "micro service",
    "sdk",
    "ms",
    "js"
  ],
  "author": "James Schimmoeller",
  "license": "Apache 2.0",
  "dependencies": {
    "documentation": "^8.0.0",
    "object-merge": "^2.5.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.1.0",
    "mocha": "^5.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
