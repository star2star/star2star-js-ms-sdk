{
  "name": "star2star-js-ms-sdk",
  "version": "3.0.3",
  "description": "Star2Star JavaScript Micro Service SDK",
  "main": "./dist/index.js",
  "scripts": {
    "init": "mkdir dist",
    "clean": "rm -rf dist && touch package-lock.json && rm package-lock.json && rm -rf node_modules && npm install",
    "test": "env MS_LOGLEVEL=silent mocha --timeout 10000 --require @babel/register",
    "trace": "env MS_LOGLEVEL=trace mocha --timeout 10000 --require @babel/register",
    "debug": "env MS_LOGLEVEL=debug env MS_LOGPRETTY=true mocha --timeout 10000 --require @babel/register",
    "build": "./node_modules/.bin/babel --config-file ./babel.config.js src -d dist",
    "postbuild": "cp src/config.json dist/config.json",
    "doc": "documentation build src/index.js -f html -o . --config documentation.yml"
  },
  "keywords": [
    "micro service",
    "sdk",
    "ms",
    "js"
  ],
  "author": "James Schimmoeller",
  "license": "Apache 2.0",
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "ajv": "6.5.3",
    "documentation": "8.0.2",
    "email-validator": "2.0.4",
    "object-merge": "2.5.1",
    "request": "2.87.0",
    "request-promise": "4.2.2",
    "uuid": "3.3.2",
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "^7.0.0",
    "assert": "^1.4.1",
    "eslint": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "mocha": "^5.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
