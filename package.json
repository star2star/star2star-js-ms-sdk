{
  "name": "star2star-js-ms-sdk",
  "version": "4.4.17",
  "description": "Star2Star JavaScript Micro Service SDK",
  "main": "./lib/index.js",
  "scripts": {
    "preclean": "npm install --no-save rimraf",
    "clean": "rimraf lib && mkdir lib && rimraf package-lock.json && rimraf node_modules && npm install",
    "test": "read -p 'env: [appDev] ' testEnv && testEnv=${testEnv:-appDev} && env-cmd --verbose -e $testEnv mocha --timeout 15000",
    "build": "npm run clean && npm run doc && babel src -d lib && npm prune --production",
    "gitTag": "git tag \"$npm_package_name\"_\"$npm_package_version\" && git push origin \"$npm_package_name\"_\"$npm_package_version\"",
    "doc": "documentation build src/index.js -f html -o . --config documentation.yml"
  },
  "keywords": [
    "micro service",
    "sdk",
    "ms",
    "js"
  ],
  "author": "Sangoma Integration Studio Team",
  "license": "MIT",
  "dependencies": {
    "@star2star/merge-deep": "0.0.2",
    "compare-versions": "4.1.4",
    "core-js": "3.30.2",
    "email-validator": "2.0.4",
    "form-data": "4.0.0",
    "node-fetch": "2.6.11",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "assert": "1.5.0",
    "documentation": "13.2.5",
    "env-cmd": "10.1.0",
    "eslint": "5.16.0",
    "jsonata": "1.8.6",
    "mocha": "8.4.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
